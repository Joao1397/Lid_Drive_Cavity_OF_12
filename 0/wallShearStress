FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0";
    object      wallShearStress;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Fixed_Walls
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    Front_And_Back
    {
        type            empty;
    }
    Moving_Wall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}


